{
  "name": "foo",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run typecheck && vite build",
    "start": "vite",
    "format": "prettier --check --ignore-path .eslintignore \"**/*.+(ts|tsx|json|css|html|md)\"",
    "format:fix": "prettier --write --ignore-path .eslintignore \"**/*.+(ts|tsx|json|css|html|md)\"",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .eslintignore",
    "lint:fix": "npm run lint -- --fix",
    "serve": "npm run build:LOC && vite preview --port 3070",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --silent --coverage",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.8.7",
    "@mui/styled-engine-sc": "5.8.0",
    "date-fns": "2.29.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.3.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@emotion/styled": "11.9.3",
    "@honkhonk/vite-plugin-svgr": "1.1.0",
    "@testing-library/cypress": "8.0.3",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "14.2.1",
    "@types/node": "18.0.1",
    "@types/react": "17.0.47",
    "@types/react-dom": "17.0.17",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-testing-library": "5.9.1",
    "jsdom": "^20.0.0",
    "prettier": "2.7.1",
    "typescript": "4.6.4",
    "vite": "2.9.13",
    "vitest": "^0.22.1"
  }
}
